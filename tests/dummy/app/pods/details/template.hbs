    {{#comp.relatedRoute 'details.related.friends' alias='Friends' svgPath='frost/tenant'}}Friends{{/comp.relatedRoute}}
    {{#comp.relatedRoute 'details.related.subscriptions' alias='Subs' svgPath='frost/service'}}Subs{{/comp.relatedRoute}}
  {{/if}}
